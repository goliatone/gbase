{
  "name": "{%= name %}",
  "version": "{%= version %}",
  "description": "{%= description %}",
  "homepage": "{%= homepage %}",
	"repository": {
		"type": "git",
		"url": "git://github.com/{%= author_name %}/{%= name %}.git"
	},
	 "bugs": {
		"url": "https://github.com/{%= author_name %}/{%= name %}/issues"
	},
	"author": {
		"name": "{%= author_name %}",
		"email": "{%= author_email %}",
		"url": "{%= author_url %}"
	},
	"licenses": [
